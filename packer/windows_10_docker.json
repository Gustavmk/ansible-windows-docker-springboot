{
  "builders": [
    {
      "type": "vmware-vmx",
      "communicator": "winrm",
      "source_path": "{{user `source_path`}}",
      "headless": false,
      "boot_wait": "5s",
      "boot_command": "",
      "winrm_username": "IEUser",
      "winrm_password": "Passw0rd!",
      "winrm_timeout": "15m",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
      "vnc_port_min": 5900,
      "vnc_port_max": 5980,
      "disk_size": 61440,
      "vmx_data": {
        "RemoteDisplay.vnc.enabled": "false",
        "RemoteDisplay.vnc.port": "5900",
        "memsize": "6144",
        "numvcpus": "4",
        "scsi0.virtualDev": "lsisas1068"
      }
    }
  ],
  "provisioners": [
    {
      "type": "powershell",
      "scripts": [
        "../vagrant-scripts/enable_winrm.ps1",
        "../vagrant-scripts/configure-ansible.ps1"
      ]
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "keep_input_artifact": false,
      "output": "windows_10_docker_{{.Provider}}.box",
      "vagrantfile_template": "vagrantfile-windows_10.template"
    }
  ],
  "variables": {
    "source_path": "../../win10boxes/MSEdge.Win10_RS1.VMWare/MSEdge - Win10_preview.vmx"
  }
}